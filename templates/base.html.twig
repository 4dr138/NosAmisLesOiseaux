<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            <link rel="icon" href = '{{ asset('assets/img/logofav.png') }}' />
            <link type = "text/css" rel="stylesheet" href="{{ asset('assets/css/style.css') }}" />
            <link rel = 'stylesheet' href = "{{ asset('bundles/easyadmin/stylesheet/bootstrap.min.css') }}" />
            <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
            <style type = 'text/css'>body {background-color: rgb(245,245,245);}</style>
        {% endblock %}
    </head>
        {% include 'header/header.html.twig' %}
    <body>
        {% block body %}{% endblock %}

        {% block javascripts %}
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
            <script src = '{{ asset('assets/js/main.js') }}' />
        {% endblock %}
    </body>
    {% include 'footer/footer.html.twig' %}
</html>
